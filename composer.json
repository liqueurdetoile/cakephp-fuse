{
    "name": "liqueurdetoile/cakephp-fuse",
    "description": "Cakephp 3/4 plugin that implements fuzzy search based on Fuse",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Liqueur de Toile",
            "homepage": "https://liqueurdetoile.com"
        }
    ],
    "keywords": [
      "database",
      "orm",
      "fuzzy",
      "fuse",
      "filter"
    ],
    "scripts": {
        "test": "phpunit",
        "stan": "phpstan analyse"
    },
    "require": {
        "php": ">=7.1",
        "cakephp/cakephp": "^3.5|^4",
        "loilo/fuse": "^3.6",
        "adbario/php-dot-notation": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.12.55"
    },
    "autoload": {
        "psr-4": {
            "Lqdt\\CakephpFuse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lqdt\\CakephpFuse\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
